{
  "manifest_version": 3,
  "name": "Web Scraper Extension",
  "version": "1.0",
  "description": "A simple web scraper extension for Chrome.",
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "19": "icons/icon19.png",
      "38": "icons/icon38.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
